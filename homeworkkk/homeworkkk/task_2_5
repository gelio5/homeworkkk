MOV A, 173	;Число для вывода
MOV B, A
 
CALL HAD	;Вызов подпрограммы
HLT	;окончание подпрограммы
 HAD:
   DIV 100
   PUSH A
   MUL 100
   SUB B,A
   MOV A,B
   DIV 10
   PUSH A
   MUL 10
   SUB B,A
   MOV A,B

 UNIT:
   MOV D,B
   POP A
   MOV C,A
   POP A
   MOV B,A
   MOV A,D
   PUSH A
   MOV A,C
   PUSH A
   MOV A,B
   MOV B,253

 A_XIT:
   CMP A,0
   JNZ OUTPUT 

 FILTRATION_1:
   INC B
   POP A
   CMP A,0
   JNZ OUTPUT

 FILTRATION_2:
   INC B
   POP A
   JMP OUTPUT 

 CHECK:
   POP A

 OUTPUT:
   ADD A, 48
   MOV [B],A
   INC B
   CMP B,0
   JZ END
   CMP B,255
   JBE CHECK  

 END:
   RET

